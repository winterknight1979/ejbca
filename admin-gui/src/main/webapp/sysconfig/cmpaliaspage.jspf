function reload() {
	document.cmpconfiguration.<%= BUTTON_RELOAD %>.click();
}

function enableFormItems() {
	var authorizedToEdit = <%=authorizedToEdit%>;
<% if (!ramode) { %>
    // ----- Client Mode -----
    var dnpartpwd = document.cmpconfiguration.<%= CHECKBOX_DNPART %>.checked;
