<%@page import="org.ejbca.core.model.authorization.AccessRulesConstants"%>
<%
   java.util.Map<String, Integer> canames = ejbcawebbean.getCANames();
   java.util.TreeMap<String, Integer> vendorcas = ejbcawebbean.getCAOptions();
   java.util.TreeMap<String, Integer> capubscas = ejbcawebbean.getCAOptions();
   int row = 0;
   ramode = cmpConfigClone.getRAMode(alias);
%>

<script type="text/javascript">
