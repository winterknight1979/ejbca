<?xml version="1.0"?>
 
<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">
 <suppressions>
 	
 	<!-- Don't check any generated files -->
	<suppress checks=".*" files="[/\\]target[/\\]"/>
	<!-- Don't care about magic numbers or package-info.java in test code -->
 	<suppress checks="MagicNumberCheck" files="[/\\]test[/\\]"/>
 	<suppress checks="JavadocPackageCheck" files="[/\\]test[/\\]"/>

 	
 </suppressions>